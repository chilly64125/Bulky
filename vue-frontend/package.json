{
  "name": "chenclan-vue-frontend",
  "version": "1.0.0",
  "description": "Vue 3 TypeScript frontend for Chen Family Ancestral Temple & Cemetery Management Platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "screenshot": "node scripts/screenshot.cjs"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "axios": "^1.6.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "pinia": "^2.1.6",
    "terser": "^5.44.1",
    "uuid": "^13.0.0",
    "vee-validate": "^4.12.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/ui": "^1.0.4",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "jsdom": "^23.0.1",
    "playwright": "^1.35.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^1.8.22"
  }
